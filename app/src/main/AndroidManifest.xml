<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamnovaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamnovaApp">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity"/>
        <activity android:name=".InfoActivity"/>
        <activity android:name=".MapActivity"/>
        <activity android:name=".RestaurantActivity"/>
        <activity android:name=".TopActivity"/>
        <activity android:name=".RecommendActivity"/>
        <activity android:name=".ReviewActivity"/>
        <activity android:name=".ReviewClicked"/>
        <activity android:name=".ReviewMainActivity"/>
        <activity android:name=".Review"/>
    </application>

</manifest>